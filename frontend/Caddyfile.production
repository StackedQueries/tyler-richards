
(blacklist) {
  @blocked {
    path *.htm *.html *.shtml *.php *.action *.do *.jsp *.jspx *.asp *.aspx
  }
  respond @blocked 444
}

tyler-richards.com {
    tls tyler@tyler-richards.com
    root * /srv
    route {
        reverse_proxy /api* api:5000
        try_files {path} {path}/ /index.html
        file_server
    }
}


