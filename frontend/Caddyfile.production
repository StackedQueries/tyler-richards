
(blacklist) {
  @blocked {
    path *.htm *.html *.shtml *.php *.action *.do *.jsp *.jspx *.asp *.aspx
  }
  respond @blocked 444
}

tyler-richards.com {
    tls tyler@tyler-richards.com
    root * /srv
    file_server
}

api.tyler-richards {
    tls tyler@tyler-richards.com
    reverse_proxy api:5000
}